{
  "version": 2,
  "buildCommand": "cd my-app && npm install && npm run build",
  "outputDirectory": "my-app/build",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/my-app/$1"
    }
  ],
  "env": {
    "EMAIL": "@email",
    "APP_PASSWORD": "@app_password",
    "REACT_APP_API_URL": "@api_url"
  }
}
